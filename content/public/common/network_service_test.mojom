module content.mojom;

struct Rule {
  string host_pattern;
  string replacement;
};

// Testing interface to the network service.
interface NetworkServiceTest {
  // Adds the given host resolver rules in the process where the network
  // service is running.
  [Sync]
  AddRules(array<Rule> rules) => ();
};

  deps = [
    "//base",
    "//ios/third_party/webkit",
    "//ios/web/navigation",
    "//ios/web/navigation:core",
    "//ios/web/public",
    "//ios/web/public/security",
    "//ios/web/web_state/ui:crw_web_view_navigation_proxy",
    "//ios/web/web_state/ui:wk_web_view_configuration_provider",

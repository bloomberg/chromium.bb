{
  "comments": [
    "Runs tests on FYI on the latest simulators."
  ],
  "xcode build version": "10a254a",
  "gn_args": [
    "additional_target_cpus=[\"x86\"]",
    "goma_dir=\"$(goma_dir)\"",
    "is_component_build=false",
    "is_debug=true",
    "symbol_level=1",
    "target_cpu=\"x64\"",
    "target_os=\"ios\"",
    "use_goma=true"
  ],
  "configuration": "Debug",
  "tests": [

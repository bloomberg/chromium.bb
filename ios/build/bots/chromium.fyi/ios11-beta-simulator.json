{
  "comments": [
    "Run tests on iOS11beta track on 64-bit iOS 11 simulators."
  ],
  "xcode build version": "9q1019a",
  "gn_args": [
    "goma_dir=\"$(goma_dir)\"",
    "additional_target_cpus=[\"x86\"]",
    "is_component_build=false",
    "is_debug=true",
    "symbol_level=1",
    "target_cpu=\"x64\"",
    "target_os=\"ios\"",
    "use_goma=true"
  ],
  "tests": [
    {
      "include": "common_tests.json",
      "device type": "iPhone X",
      "os": "11.4",
      "host os": "Mac-10.13",
      "xcode build version": "9q1019a"
    },
    {
      "include": "common_tests.json",
      "device type": "iPad Air",
      "os": "11.4",
      "host os": "Mac-10.13",
      "xcode build version": "9q1019a"
    },
    {
      "include": "common_tests.json",
      "device type": "iPhone 5s",
      "os": "11.4",
      "host os": "Mac-10.13",
      "xcode build version": "9q1019a"
    },
    {
      "include": "eg_tests.json",
      "device type": "iPhone X",
      "os": "11.4",
      "host os": "Mac-10.13",
      "xcode build version": "9q1019a"
    },
    {
      "include": "eg_tests.json",
      "device type": "iPad Air",
      "os": "11.4",
      "host os": "Mac-10.13",
      "xcode build version": "9q1019a"
    },
    {
      "include": "eg_tests.json",
      "device type": "iPhone 5s",
      "os": "11.4",
      "host os": "Mac-10.13",
      "xcode build version": "9q1019a"
    }
  ]
}

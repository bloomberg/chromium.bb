{
  "comments": [
    "Tests for 64-bit iOS 10.0 simulators."
  ],
  "xcode version": "8.0",
  "gn_args": [
    "goma_dir=\"$(goma_dir)\"",
    "is_component_build=false",
    "is_debug=true",
    "target_cpu=\"x64\"",
    "target_os=\"ios\"",
    "use_goma=true"
  ],
  "configuration": "Debug",
  "sdk": "iphonesimulator10.0",
  "tests": [
    {
      "app": "ios_chrome_integration_egtests",
      "device type": "iPhone 6s",
      "os": "10.0",
      "xcode version": "8.0",
      "xctest": true
    },
    {
      "app": "ios_chrome_payments_egtests",
      "test args": [
        "--enable-features=WebPayments"
      ],
      "device type": "iPhone 6s",
      "os": "10.0",
      "xcode version": "8.0",
      "xctest": true
    },
    {
      "app": "ios_chrome_reading_list_egtests",

{
  "comments": [
    "Tests for 64-bit iOS 10, 11.0 phone and tablet simulators on FYI",
    "EarlGreyiOS."
  ],
  "xcode version": "9.0",
  "gn_args": [
    "goma_dir=\"$(goma_dir)\"",
    "is_component_build=false",
    "is_debug=true",
    "target_cpu=\"x64\"",
    "target_os=\"ios\"",
    "use_goma=true"
  ],
  "configuration": "Debug",
  "sdk": "iphonesimulator11.0",
  "tests": [
    {
      "app": "ios_chrome_integration_egtests",
      "test args": [
        "--enable-features=CredentialManager"
      ],
      "device type": "iPhone 6s",
      "os": "11.0",
      "xcode version": "9.0",
      "xctest": true
    },
    {
      "app": "ios_chrome_integration_egtests",
      "test args": [
        "--enable-features=CredentialManager"
      ],
      "device type": "iPad Air 2",
      "os": "10.0",
      "xcode version": "9.0",
      "xctest": true
    }
  ]
}

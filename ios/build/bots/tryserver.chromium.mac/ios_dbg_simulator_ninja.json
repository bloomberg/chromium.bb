{
  "owners": [
    "smut"
  ],
  "comments": [
    "Tests for 32- and 64-bit iOS 9.0 simulators."
  ],
  "xcode version": "7.0",
  "GYP_DEFINES": {
    "OS": "ios",
    "chromium_ios_signing": "0",
    "gomadir": "$(goma_dir)",
    "target_subarch": "both",
    "use_goma": "1"
  },
  "mb_type": "gyp",
  "compiler": "ninja",
  "configuration": "Debug",
  "sdk": "iphonesimulator9.0",
  "tests": [
    {
      "include": "common_tests.json",
      "device type": "iPhone 5s",
      "os": "9.0"
    },
    {
      "include": "common_tests.json",
      "device type": "iPad Retina",
      "os": "9.0"
    },
    {
      "include": "common_tests.json",
      "device type": "iPhone 5s",
      "os": "8.3"
    },
    {
      "include": "common_tests.json",
      "device type": "iPad Retina",
      "os": "8.3"
    }
  ]
}

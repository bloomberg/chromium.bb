{
  "comments": [
    "Runs tests on 64-bit iOS 10 and 11 tests",
    "on iPad, iPhone, @3x, and @2x on main and CQ ios-simulator.",
    "Note: Xcode 9.2 requires Mac OS 10.12.6 or higher, hence 'host os'."
  ],
  "xcode build version": "9C40b",
  "gn_args": [
    "additional_target_cpus=[\"x86\"]",
    "goma_dir=\"$(goma_dir)\"",
    "is_component_build=false",
    "is_debug=true",
    "symbol_level=1",
    "target_cpu=\"x64\"",
    "target_os=\"ios\"",
    "use_goma=true"
  ],
  "additional_compile_targets": [
    "all"
  ],
  "configuration": "Debug",
  "sdk": "iphonesimulator11.2",
  "tests": [
    {
      "include": "screen_size_dependent_tests.json",
      "device type": "iPhone 6s Plus",
      "os": "11.2",
      "dimensions": [
        {
          "os": "Mac-10.12",
          "pool": "Chrome"
        },
        {
          "os": "Mac-10.13",
          "pool": "Chrome"
        }
      ],
      "priority": 29
    },
    {
      "include": "screen_size_dependent_tests.json",
      "device type": "iPhone 6s",
      "os": "11.2",
      "dimensions": [
        {
          "os": "Mac-10.12",
          "pool": "Chrome"
        },
        {
          "os": "Mac-10.13",
          "pool": "Chrome"
        }
      ],
      "priority": 29
    },
    {
      "include": "common_tests.json",
      "device type": "iPhone 6s",
      "os": "11.2",
      "dimensions": [
        {
          "os": "Mac-10.12",
          "pool": "Chrome"
        },
        {
          "os": "Mac-10.13",
          "pool": "Chrome"
        }
      ],
      "priority": 29
    },
    {
      "include": "screen_size_dependent_tests.json",
      "device type": "iPad Air 2",
      "os": "11.2",
      "dimensions": [
        {
          "os": "Mac-10.12",
          "pool": "Chrome"
        },
        {
          "os": "Mac-10.13",
          "pool": "Chrome"
        }
      ],
      "priority": 29
    },
    {

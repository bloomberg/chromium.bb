{
  "comments": [
    "Runs tests on iOS 12.2 and 13.1 on iPad, iPhone, @3x, and @2x on",
    "main and CQ ios-simulator.",
    "Note: Xcode 11 requires Mac OS 10.14.4 or higher, hence 'host os'."
  ],
  "xcode build version": "11a1027",
  "gn_args": [
    "goma_dir=\"$(goma_dir)\"",
    "is_component_build=false",
    "is_debug=true",
    "symbol_level=1",
    "target_cpu=\"x64\"",
    "target_os=\"ios\"",
    "use_goma=true"
  ],
  "additional_compile_targets": [
    "all"
  ],
  "configuration": "Debug",
  "tests": [
    {
      "include": "common_tests.json",
      "device type": "iPhone 6s",
      "os": "12.2",
      "host os": "Mac-10.14.6",
      "pool":"chromium.tests",
      "priority": 30
    },
    {
      "include": "eg_cq_tests.json",
      "device type": "iPhone 6s",
      "os": "12.2",
      "host os": "Mac-10.14.6",
      "pool":"chromium.tests",
      "priority": 30
    },
    {
      "include": "screen_size_dependent_tests.json",
      "device type": "iPhone 6s Plus",
      "os": "13.1",
      "host os": "Mac-10.14.6",
      "pool":"chromium.tests",
      "priority": 30
    },
    {
      "include": "screen_size_dependent_tests.json",
      "device type": "iPhone 6s",
      "os": "13.1",
      "host os": "Mac-10.14.6",
      "pool":"chromium.tests",
      "priority": 30
    },
    {
      "include": "screen_size_dependent_tests.json",
      "device type": "iPhone SE",
      "os": "13.1",
      "host os": "Mac-10.14.6",
      "pool":"chromium.tests",
      "priority": 30
    },
    {
      "include": "common_tests.json",
      "device type": "iPhone 6s",
      "os": "13.1",
      "host os": "Mac-10.14.6",
      "pool":"chromium.tests",
      "priority": 30
    },
    {
      "include": "screen_size_dependent_tests.json",
      "device type": "iPad Air 2",
      "os": "13.1",
      "host os": "Mac-10.14.6",
      "pool":"chromium.tests",
      "priority": 30
    },
    {
      "include": "eg_cq_tests.json",
      "device type": "iPhone 6s",
      "os": "13.1",
      "host os": "Mac-10.14.6",
      "pool":"chromium.tests",
      "priority": 30
    }
  ]
}

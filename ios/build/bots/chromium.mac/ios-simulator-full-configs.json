      "shards": 1,
      "shard size": 8,
      "dimensions": [
        {
          "os": "Mac-10.12",
          "pool": "Chrome"
        },
        {
          "os": "Mac-10.13",
          "pool": "Chrome"
        }
      ]
    },
    {
      "include": "eg_tests.json",
      "device type": "iPhone X",
      "os": "11.2",

source_set("mailto") {
  configs += [ "//build/config/compiler:enable_arc" ]
  sources = [
    "mailto_handler_provider.h",
    "mailto_handler_provider.mm",
  ]
  deps = [ "//base" ]
}

source_set("test_support") {
  configs += [ "//build/config/compiler:enable_arc" ]
  testonly = true
  sources = [
    "test_mailto_handler_provider.h",
    "test_mailto_handler_provider.mm",
  ]
  deps = [ ":mailto" ]
}

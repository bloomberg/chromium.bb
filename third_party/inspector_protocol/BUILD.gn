import("//build/config/jumbo.gni")

jumbo_component("crdtp") {
  sources = [
    "crdtp/bindings.cc",
    "crdtp/bindings.h",
    "crdtp/encoding.cc",
    "crdtp/encoding.h",
    "crdtp/export.h",
  ]

  defines = [ "CRDTP_IMPLEMENTATION" ]

  if (!is_debug) {
    configs -= [ "//build/config/compiler:default_optimization" ]
    configs += [ "//build/config/compiler:optimize_max" ]
  }
}

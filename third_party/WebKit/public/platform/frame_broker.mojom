module blink.mojom;

import "mojo/common/time.mojom";

// Interface for communicating with the browser-side frame host.
interface FrameBroker {
  // Corresponds to the first paint Web Platform API. See
  // https://github.com/WICG/paint-timing.
  OnFirstPaint(mojo.common.mojom.TimeDelta time_to_first_paint);
};

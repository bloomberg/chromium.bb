// Specification at:
// https://w3c.github.io/permissions/

enum PermissionName {
    "geolocation",
    "notifications",
    "push-notifications",
    "midi-sysex"
};

[
    Exposed=(Window,Worker),
    GarbageCollected,
    RuntimeEnabled=Permissions
] interface Permissions {
    [CallWith=ScriptState] static Promise<PermissionStatus> query(PermissionName permission);
};

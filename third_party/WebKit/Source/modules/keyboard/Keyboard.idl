[
    Exposed=Window,
    RuntimeEnabled=KeyboardLock,
    SecureContext
] interface Keyboard {
    [CallWith=ScriptState] Promise<void> lock(optional sequence<DOMString> keyCodes = []);
    void unlock();
};

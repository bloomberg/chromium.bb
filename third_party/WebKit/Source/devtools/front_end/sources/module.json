            "bindings": [
                {
                    "platform": "windows,linux",
                    "shortcut": "F10 Ctrl+'"
                },
                {
                    "platform": "mac",
                    "shortcut": "F10 Meta+'"
                }
            ]
        },
        {
            "type": "@UI.ActionDelegate",

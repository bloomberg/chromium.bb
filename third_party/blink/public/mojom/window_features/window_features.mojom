module blink.mojom;

struct WindowFeatures {
    float x;
    bool has_x = false;

    float y;
    bool has_y = false;

    float width;
    bool has_width = false;

    float height;
    bool has_height = false;

    bool menu_bar_visible = true;
    bool status_bar_visible = true;
    bool tool_bar_visible = true;
    bool scrollbars_visible = true;
};

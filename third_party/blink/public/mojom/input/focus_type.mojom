module blink.mojom;

enum FocusType {
  // Element::focus(), etc.
  kNone = 0,
  // Sequential navigation with TAB, or Shift + TAB.
  kForward,
  kBackward,
  // Spatial navigation.
  kSpatialNavigation,
  // Mouse press
  kMouse,
  // Access key
  kAccessKey,
  // Re-focus by a page focus
  kPage,
};

// ARIA reflection
// https://github.com/w3c/aria/pull/708/files
[
    NoInterfaceObject,
    RuntimeEnabled=AccessibilityObjectModel
] interface AccessibilityRole {
  [CEReactions, Reflect] attribute DOMString? role;
};

Element implements AccessibilityRole;

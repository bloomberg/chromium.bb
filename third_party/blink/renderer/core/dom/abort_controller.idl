// https://dom.spec.whatwg.org/#interface-abortcontroller

[
    Constructor,
    ConstructorCallWith=ExecutionContext,
    Exposed=(Window,Worker),
    Measure
] interface AbortController {
    [SameObject] readonly attribute AbortSignal signal;

    void abort();
};

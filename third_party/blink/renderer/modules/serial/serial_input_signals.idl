// https://wicg.github.io/serial

dictionary SerialInputSignals {
  // DCD (Data Carrier Detect) or RLSD (Receive Line Signal Detect)
  required boolean dcd;
  // CTS (Clear to Send)
  required boolean cts;
  // RI (Ring Indicator)
  required boolean ri;
  // DSR (Data Set Ready)
  required boolean dsr;
};

// https://w3c.github.io/web-nfc/#the-nfcpushoptions-dictionary

enum NFCPushTarget { "tag", "peer", "any" };

dictionary NFCPushOptions {
    NFCPushTarget target = "any";
    unrestricted double timeout; // in ms
    boolean ignoreRead = true;
};

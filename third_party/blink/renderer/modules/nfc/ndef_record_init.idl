enum NDEFRecordType { "empty", "text", "url", "json", "opaque" };

typedef (DOMString or unrestricted double or ArrayBuffer or Dictionary) NDEFRecordData;

dictionary NDEFRecordInit {
    NDEFRecordType recordType;
    USVString mediaType;
    NDEFRecordData data;
};

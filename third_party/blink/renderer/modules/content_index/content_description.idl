enum ContentCategory {
    "homepage",
    "article",
    "video",
    "audio",
};

dictionary ContentDescription {
    DOMString id;
    DOMString title;
    DOMString description;
    ContentCategory category;
    USVString iconUrl;
    USVString launchUrl;
};

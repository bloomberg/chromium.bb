{
  "manifest_version":  1,
  "name": "mojo:ash",
  "display_name": "Ash Window Manager and Shell",
  "capabilities": {
    "provided": {
      "mus:window_manager" : [ "ui::mojom::AcceleratorRegistrar" ]
    },
    "required": {
      "*": { "classes": [ "app" ] },
      "mojo:ui": { "interfaces": [ "ui::mojom::WindowManagerWindowTreeFactory" ] }
    }
  }
}

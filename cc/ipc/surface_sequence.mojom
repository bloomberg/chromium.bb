module cc.mojom;

// A per-surface-namespace sequence number that's used to coordinate
// dependencies between frames. A sequence number may be satisfied once, and
// may be depended on once.
struct SurfaceSequence {
  uint32 client_id;
  uint32 sequence;
};
 

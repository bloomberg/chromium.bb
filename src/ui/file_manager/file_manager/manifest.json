  },
  "permissions": [
    "chrome://extension-icon/",
    "chrome://app-icon/",
    "chrome://resources/",
    "chrome://theme/",
    "clipboardRead",
    "clipboardWrite",
    "commandLinePrivate",
    "contextMenus",
    "echoPrivate",
    "fileManagerPrivate",
    {
      "fileSystem": ["requestFileSystem", "write"]
    },
    "fullscreen",
    "https://*.googleusercontent.com/",
    "https://docs.google.com/",
    "https://drive.google.com/",
    "https://www.google-analytics.com/",
    "metricsPrivate",
    "notifications",
    "power",
    "storage",
    "syncFileSystem",
    "unlimitedStorage",
    "wallpaper",
    "webview"
  ],
  "webview": {
    "partitions": [{
      "name": "trusted",
      "accessible_resources": [
        "untrusted_resources/files_audio_content.*",
        "untrusted_resources/files_img_content.*",
        "untrusted_resources/files_media_content.js",
        "untrusted_resources/files_text_content.*",
        "untrusted_resources/files_video_content.*",
        "untrusted_resources/url_constants.js"
      ]
    }]
  },
  "file_handlers": {
    "mount-archive": {
      "extensions": [
        "zip",
        "rar",
        "7z",
        "crx",
        "iso",
        "tar",
        "tbz",
        "tbz2",
        "tgz",
        "tlzma",
        "txz",
        "bz",
        "bz2",
        "gz",
        "lzma",
        "xz"
      ]
    },
    "view-pdf": {
      "extensions": [
        "pdf"
      ],
      "types": [
        "application/pdf"
      ]
    },
    "view-in-browser": {
      "extensions": [
        "htm",
        "html",
        "mht",
        "mhtml",
        "shtml",
        "xht",
        "xhtml",
        "svg",
        "txt"
      ],
      "types": [
        "text/plain"
      ]
    },
    "open-hosted-generic": {
      "extensions": [
        "gdraw",
        "gtable",
        "gform",
        "gmaps",
        "gsite",
        "glink"
      ]
    },
    "open-hosted-gdoc": {
      "extensions": [
        "gdoc"
      ]
    },
    "open-hosted-gsheet": {
      "extensions": [
        "gsheet"
      ]
    },
    "open-hosted-gslides": {
      "extensions": [
        "gslides"
      ]
    },
    "open-web-drive-office-word": {
      "extensions": [
        "doc",
        "docx"
      ]
    },
    "open-web-drive-office-excel": {
      "extensions": [
        "xls",
        "xlsx"
      ]
    },
    "open-web-drive-office-powerpoint": {
      "extensions": [
        "ppt",
        "pptx"
      ]
    },
    "install-linux-package": {
      "extensions": [
        "deb"
      ]
    },
    "import-crostini-image": {
      "extensions": [
        "tini"
      ]
    },
    "select": {
      "extensions": ["*"]
    },
    "open": {
      "extensions": ["*"],
      "include_directories": true
    }
  },

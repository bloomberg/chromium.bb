  "manifest_version": 2,
  "name": "Files",
  "version": "3.0",
  "description": "File Manager",
  "incognito" : "split",
  "icons": {
    "16": "common/images/icon16.png",
    "32": "common/images/icon32.png",
    "48": "common/images/icon48.png",
    "64": "common/images/icon64.png",
    "96": "common/images/icon96.png",
    "128": "common/images/icon128.png",

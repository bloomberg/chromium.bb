// gpu/config/gpu_extra_info.h
module gpu.mojom;

// gpu::ANGLEFeature
struct ANGLEFeature {
  string name;
  string category;
  string description;
  string bug;
  string status;
};

// gpu:GpuExtraInfo
struct GpuExtraInfo {
  // List of features queried from ANGLE
  array<ANGLEFeature> angle_features;
};

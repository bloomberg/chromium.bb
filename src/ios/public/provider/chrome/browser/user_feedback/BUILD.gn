source_set("user_feedback") {
  configs += [ "//build/config/compiler:enable_arc" ]
  sources = [
    "user_feedback_provider.h",
    "user_feedback_provider.mm",
  ]
  deps = [ "//base" ]
}

source_set("test_support") {
  configs += [ "//build/config/compiler:enable_arc" ]
  testonly = true
  sources = [
    "test_user_feedback_provider.h",
    "test_user_feedback_provider.mm",
  ]
  deps = [
    ":user_feedback",
    "//base",
  ]
}

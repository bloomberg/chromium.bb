module network.mojom;

struct CSPSource {
  string scheme;
  string host;
  int32 port = -1;
  string path;
  bool is_host_wildcard = false;
  bool is_port_wildcard = false;
  bool allow_self = false;
};

struct CSPSourceList {
  array<CSPSource> sources;
};

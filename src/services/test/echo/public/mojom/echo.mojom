module echo.mojom;

// Echos its input.
interface EchoService {
  // Echos the passed-in string.
  EchoString(string input) => (string echoed_input);

  // Causes the service to disconnect itself.
  Quit();

  // Forcibly crashes the service.
  Crash();
};

@hex:
  # Subtract a 32-bit bounded value from esp and then update rsp.
  # 0000000000000000: 81 ec 20 00 00 00                            sub %esp, 0x20
  # 0000000000000006: 4c 01 fc                                     add %rsp, %r15
  81 ec 20 00 00 00 \\
  4c 01 fc
@dis:
     0:	81 ec 20 00 00 00                            	sub    $0x20,%esp
     6:	4c 01 fc                                     	add    %r15,%rsp
@rdfa_output:
  return code: 0
@spec:
  SAFE

@hex:
  # This file tests how we handle a 16-bit direct call. In particular, it tests
  # whether we actually disallow prefix 66 on the direct call.
  #
  # Pad with nops, so that call is bundle aligned.
  90 90 90 90 90 90 90 90
  90 90 90 90 90 90 90 90
  90 90 90 90 90 90 90 90
  90 90 90
  
  # Define 16-bit direct call.
  66 e8 F0 FF
  
  # Padding in case the parser of the previous instruction recognizes
  # the wrong byte length for the jump constant.
  90 90
@dis:
     0:	90                                           	nop
     1:	90                                           	nop
     2:	90                                           	nop
     3:	90                                           	nop
     4:	90                                           	nop
     5:	90                                           	nop
     6:	90                                           	nop
     7:	90                                           	nop
     8:	90                                           	nop
     9:	90                                           	nop
     a:	90                                           	nop
     b:	90                                           	nop
     c:	90                                           	nop
     d:	90                                           	nop
     e:	90                                           	nop
     f:	90                                           	nop
    10:	90                                           	nop
    11:	90                                           	nop
    12:	90                                           	nop
    13:	90                                           	nop
    14:	90                                           	nop
    15:	90                                           	nop
    16:	90                                           	nop
    17:	90                                           	nop
    18:	90                                           	nop
    19:	90                                           	nop
    1a:	90                                           	nop
    1b:	66 e8 f0 ff                                  	callw  0xf
    1f:	90                                           	nop
    20:	90                                           	nop
@rdfa_output:
  1b: unrecognized instruction
  return code: 1
@spec:
  1b: ('16-bit jumps are disallowed', Instruction(0x1b: 66 e8 f0 ff  callw 0xf))

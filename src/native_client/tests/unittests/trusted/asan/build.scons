Import('env')

if not env.Bit('asan'):
  Return()

asan_test_exe = env.ComponentProgram('asan_test', 'asan_test.c')

node = env.CommandTest(
    'asan_test.out',
    command=[asan_test_exe],
    exit_status='1',
    )
env.AddNodeToTestSuite(node, ['small_tests'], 'run_asan_test')

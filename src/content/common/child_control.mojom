module content.mojom;

interface ChildControl {
  // Tell the child process that it's safe to shutdown.
  ProcessShutdown();

  // Tell the child process to begin or end IPC message logging.
  [EnableIf=ipc_logging]
  SetIPCLoggingEnabled(bool on);
};

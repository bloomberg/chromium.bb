      document_policy_name: "unsized-media",
      value_name: "",
      value_type: "Bool",
      default_value: "true",
      depends_on: ["UnsizedMediaPolicy"],
    },
  {
      name: "LayoutAnimations",
      document_policy_name: "layout-animations",

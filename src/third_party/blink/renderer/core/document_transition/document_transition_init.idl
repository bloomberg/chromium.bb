enum RootTransitionType {
  "none",
  "cover-down",
  "cover-left",
  "cover-right",
  "cover-up",
  "explode",
  "fade",
  "implode",
  "reveal-down",
  "reveal-left",
  "reveal-right",
  "reveal-up"
};

dictionary DocumentTransitionInit {
  RootTransitionType rootTransition = "none";
  DOMTimeStamp duration = 0;
};

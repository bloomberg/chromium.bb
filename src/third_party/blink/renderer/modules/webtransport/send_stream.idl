// https://wicg.github.io/web-transport/#send-stream

[ Exposed=(Window,Worker),
  RuntimeEnabled=WebTransport ]
interface SendStream : WritableStream {
  // These attributes and method are for backwards compatibility.
  // TODO(ricea): Remove them once the origin trial is over.
  readonly attribute SendStream writable;
  readonly attribute Promise<StreamAbortInfo> writingAborted;
  void abortWriting(optional StreamAbortInfo abortInfo = {});
};

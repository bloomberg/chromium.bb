// https://w3c.github.io/webrtc-quic/#dom-rtcquicrole
enum RTCQuicRole {
  "auto",
  "client",
  "server",
};

// https://w3c.github.io/webrtc-quic/#rtcquicparameters*
dictionary RTCQuicParameters {
  RTCQuicRole role = "auto";
  sequence<RTCDtlsFingerprint> fingerprints;
};

module blink.mojom;

// Enumerates the state of which the touch point can be in.
enum TouchState {
  kStateUndefined,
  kStateReleased,
  kStatePressed,
  kStateMoved,
  kStateStationary,
  kStateCancelled,
  kStateMax = kStateCancelled
};


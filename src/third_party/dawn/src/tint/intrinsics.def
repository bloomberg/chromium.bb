conv vec3<T: u32, U: scalar_no_u32>(vec3<U>) -> vec3<u32>
conv vec3<T: bool, U: scalar_no_bool>(vec3<U>) -> vec3<bool>

conv vec4<T: f32, U: scalar_no_f32>(vec4<U>) -> vec4<f32>

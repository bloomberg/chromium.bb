#!/bin/bash

# The following symbols (past the first five) are taken from the public headers.
# A list of the latter should be available Makefile.am/libdrm_omap*HEADERS

FUNCS=$(nm -D --format=bsd --defined-only ${1-.libs/libdrm_omap.so} | awk '{print $3}'| while read func; do
( grep -q "^$func$" || echo $func )  <<EOF
__bss_start
_edata
_end
_fini
_init
omap_bo_cpu_fini
omap_bo_cpu_prep
omap_bo_del
omap_bo_dmabuf
omap_bo_from_dmabuf
omap_bo_from_name
omap_bo_get_name
omap_bo_handle
omap_bo_map
omap_bo_new
omap_bo_new_tiled
omap_bo_ref
omap_bo_size
omap_device_del
omap_device_new
omap_device_ref
omap_get_param
omap_set_param
EOF
done)

test ! -n "$FUNCS" || echo $FUNCS
test ! -n "$FUNCS"

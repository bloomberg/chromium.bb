  outputs = [
    "$target_gen_dir/src/net/third_party/quic/core/proto/cached_network_parameters.pb.cc",
    "$target_gen_dir/src/net/third_party/quic/core/proto/cached_network_parameters.pb.h",
    "$target_gen_dir/src/net/third_party/quic/core/proto/source_address_token.pb.cc",
    "$target_gen_dir/src/net/third_party/quic/core/proto/source_address_token.pb.h",
  ]

  args = rebase_path(sources, get_path_info(sources[0], "dir"))
  args += [
    "--protoc",
    "./protoc",
    "--proto-in-dir",
    rebase_path(get_path_info(sources[0], "dir"), root_build_dir),
    "--cc-out-dir",
    rebase_path(get_path_info(outputs[0], "dir"), root_build_dir),
  ]

  deps = [
    "src/third_party/protobuf:protoc",
  ]
}


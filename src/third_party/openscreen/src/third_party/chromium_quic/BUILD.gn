action("proto_gen") {
  script = "src/tools/protoc_wapper/protoc_wrapper.py"
  sources = [
    "src/net/third_party/quic/core/proto/cached_network_parameters.proto",
    "src/net/third_party/quic/core/proto/source_address_token.proto",

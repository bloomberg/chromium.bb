group("unittests") {
  deps = [
    ":copy_debug_check",
    ":copy_to_gen",
    "../../front_end/ui/legacy:copy_stylesheets_for_server",
    "front_end",
    "inspector_overlay",
  ]
}

copy("copy_to_gen") {
  sources = [ "karma.conf.js" ]
  outputs = [ "$target_gen_dir/karma.conf.js" ]
}

copy("copy_debug_check") {
  sources = [ "debug-check.js" ]
  outputs = [ "$target_gen_dir/debug-check.js" ]
}

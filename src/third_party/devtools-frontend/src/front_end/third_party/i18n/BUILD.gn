import("../../../scripts/build/ninja/devtools_entrypoint.gni")
import("../../../scripts/build/ninja/devtools_module.gni")

devtools_module("i18n") {
  sources = [
    "i18n-impl.ts",
    "locales.ts",
    "localized-string-set.ts",
  ]

  deps = [ "../intl-messageformat:bundle" ]
}

devtools_entrypoint("bundle") {
  entrypoint = "i18n.ts"
  deps = [ ":i18n" ]

  visibility = [ "*" ]
}

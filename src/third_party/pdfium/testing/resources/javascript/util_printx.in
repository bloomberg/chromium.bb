function TestOneFormat(fmt, src) {
  var title = "('" + fmt + "', '" + src + "')";
  try {
    app.alert(title +  " => '" + util.printx(fmt, src) + "'");
  }
  catch (e) {
    app.alert(title + ": Caught error: " + e);
  }
}


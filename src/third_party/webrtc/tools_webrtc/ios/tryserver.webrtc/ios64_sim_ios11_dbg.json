{
  "comments": [
    "Tests for 64-bit iOS simulators."
  ],
  "xcode build version": "9C40b",
  "gn_args": [
    "goma_dir=\"$(goma_dir)\"",
    "is_component_build=false",
    "is_debug=true",
    "target_cpu=\"x64\"",
    "target_os=\"ios\"",
    "use_goma=true"
  ],
  "use_analyze": false,
  "additional_compile_targets": [ "all" ],
  "configuration": "Debug",
  "sdk": "iphonesimulator11.0",
  "tests": [
    {
      "include": "common_tests.json",
      "device type": "iPhone 7",
      "os": "11.0",
      "dimensions": [
        {
          "os": "Mac-10.12",
          "pool": "Chrome"
        },
        {
          "os": "Mac-10.13",
          "pool": "Chrome"
        }
      ]
    }
  ]
}

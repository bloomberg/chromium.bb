// RUN: xla-gpu-opt %s | FileCheck %s
HloModule Sign
ENTRY %Sign (val: f32[2,2]) -> f32[2,2] {
  %val = f32[2,2]{1,0} parameter(0)
  ROOT %sign = f32[2,2]{1,0} sign(f32[2,2]{1,0} %val)
}


// RUN: xla-gpu-opt %s | FileCheck %s
HloModule Sqrt

ENTRY %Sqrt (x: f32[2,2]) -> f32[2,2] {
  %x = f32[2,2]{1,0} parameter(0)
  ROOT %sqrt = f32[2,2]{1,0} sqrt(f32[2,2]{1,0} %x)
}

// CHECK: func @sqrt(%[[ARG0:.*]]: [[TYPE:.*]], %[[ARG1:.*]]: [[TYPE]]) {
// CHECK:   "xla_lhlo.sqrt"(%[[ARG0]], %[[ARG1]]) : ([[TYPE]], [[TYPE]]) -> ()
// CHECK: }


{
  "name": "Tab Capture Example",
  "description": "Capture a tab and play in a <video> element in a separate tab.",
  "version": "1",
  "manifest_version": 2,
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "tabCapture"
  ]
}

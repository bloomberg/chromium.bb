# This target builds the updater executable and unittests.
group("linux") {
  deps = [ ":updater" ]
}

executable("updater") {
  sources = [ "main.cc" ]

  deps = [
    "//chrome/updater:base",
    "//chrome/updater:constants_prod",
  ]
}

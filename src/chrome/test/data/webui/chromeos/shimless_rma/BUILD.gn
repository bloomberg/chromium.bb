import("//third_party/closure_compiler/compile_js.gni")

js_type_check("closure_compile") {
  is_polymer3 = true
  closure_flags = default_closure_args + [
                    "browser_resolver_prefix_replacements=\"chrome://shimless-rma/=../../ash/content/shimless_rma/resources/\"",
                    "js_module_root=../../chrome/test/data/webui/",
                    "js_module_root=./gen/chrome/test/data/webui/",
                  ]
  deps = [
    ":fake_shimless_rma_service_test",
    ":onboarding_update_page_test",
    ":shimless_rma_app_test",
    ":shimless_rma_unified_test",
  ]
}

js_library("fake_shimless_rma_service_test") {
  deps = [
    "../..:chai_assert",
    "//ash/content/shimless_rma/resources:fake_shimless_rma_service",
  ]
  externs_list = [ "$externs_path/mocha-2.5.js" ]
}

js_library("shimless_rma_app_test") {
  deps = [
    "../..:chai_assert",
    "//ash/content/shimless_rma/resources:shimless_rma",
  ]
  externs_list = [ "$externs_path/mocha-2.5.js" ]
}

js_library("onboarding_update_page_test") {
  deps = [
    "../..:chai_assert",
    "//ash/content/shimless_rma/resources:onboarding_update_page",
  ]
  externs_list = [ "$externs_path/mocha-2.5.js" ]
}

js_library("shimless_rma_unified_test") {
  deps = []
  externs_list = [ "$externs_path/mocha-2.5.js" ]
}

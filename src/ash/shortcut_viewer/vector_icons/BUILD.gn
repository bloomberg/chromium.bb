    "ksv_arrow_up.icon",
    "ksv_brightness_down.icon",
    "ksv_brightness_up.icon",
    "ksv_browser_back.icon",
    "ksv_browser_forward.icon",
    "ksv_fullscreen.icon",
    "ksv_mute.icon",
    "ksv_overview.icon",
    "ksv_privacy_screen_toggle.icon",
    "ksv_reload.icon",
    "ksv_volume_down.icon",
    "ksv_volume_up.icon",

{
  "One Buildbot Step Test Builder": {
    "isolated_scripts": [
      {
        "args": [
          "--gtest-benchmark-name",
          "load_library_perf_tests",
          "--non-telemetry=true",
          "--migrated-test=true"
        ],
        "isolate_name": "load_library_perf_tests",
        "merge": {
          "script": "//tools/perf/process_perf_results.py"
        },
        "name": "load_library_perf_tests",
        "override_compile_targets": [
          "load_library_perf_tests"
        ],
        "swarming": {
          "can_use_on_swarming_builders": true,
          "dimension_sets": [
            {
              "gpu": "none",
              "os": "Linux",
              "pool": "chrome.tests.perf-fyi"
            }
          ],
          "expiration": 7200,
          "hard_timeout": 25200,
          "ignore_task_failure": false,
          "io_timeout": 1800,
          "shards": 1,
          "upload_test_results": true
        },
        "trigger_script": {
          "args": [
            "--multiple-dimension-script-verbose",
            "True"
          ],
          "script": "//testing/trigger_scripts/perf_device_trigger.py"
        }
      },
      {
        "args": [
          "-v",
          "--browser=reference",
          "--upload-results",
          "--xvfb",
          "--run-ref-build",
          "--test-shard-map-filename=benchmark_bot_map.json"
        ],
        "isolate_name": "telemetry_perf_tests_without_chrome",
        "merge": {
          "script": "//tools/perf/process_perf_results.py"
        },
        "name": "telemetry_perf_tests_without_chrome",
        "override_compile_targets": [
          "telemetry_perf_tests_without_chrome"
        ],
        "swarming": {
          "can_use_on_swarming_builders": true,
          "dimension_sets": [
            {
              "gpu": "none",
              "os": "Linux",
              "pool": "chrome.tests.perf-fyi"
            }
          ],
          "expiration": 7200,
          "hard_timeout": 25200,
          "ignore_task_failure": false,
          "io_timeout": 1800,
          "shards": 3,
          "upload_test_results": true
        },
        "trigger_script": {
          "args": [
            "--multiple-dimension-script-verbose",
            "True"
          ],
          "script": "//testing/trigger_scripts/perf_device_trigger.py"
        }
      }
    ]
  },

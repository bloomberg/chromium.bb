{
  "name": "content_capabilities test",
  "version": "1",
  "manifest_version": 2,
  "content_capabilities": {
    "matches": [
      "https://*.co.uk/",
      "https://*.appspot.com/",
      "<all_urls>",
      "https://example.com/"
    ],
    "permissions": [
      "clipboardRead",
      "clipboardWrite",
      "unlimitedStorage"
    ]
  }
}

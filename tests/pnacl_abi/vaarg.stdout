target triple = "armv7-none-linux-gnueabi"

define i32 @foo(i8 zeroext %fmt, ...) nounwind {
entry:
  %ap = alloca [4 x i32], align 4
  %ap12 = bitcast [4 x i32]* %ap to i8*
  call void @llvm.va_start(i8* %ap12)
  switch i8 %fmt, label %bb10 [
    i8 112, label %bb
    i8 105, label %bb5
    i8 100, label %bb8
  ]

bb:                                               ; preds = %entry
  %ap3 = getelementptr inbounds [4 x i32]* %ap, i32 0, i32 0
  %0 = va_arg i32* %ap3, i32*
  %1 = load i32* %0, align 4
  ret i32 %1

bb5:                                              ; preds = %entry
  %ap6 = getelementptr inbounds [4 x i32]* %ap, i32 0, i32 0
  %2 = va_arg i32* %ap6, i32
  ret i32 %2

bb8:                                              ; preds = %entry
  %ap9 = getelementptr inbounds [4 x i32]* %ap, i32 0, i32 0
  %3 = va_arg i32* %ap9, double
  %4 = fptosi double %3 to i32
  ret i32 %4

bb10:                                             ; preds = %entry
  call void @llvm.va_end(i8* %ap12)
  ret i32 -1
}

declare void @llvm.va_start(i8*) nounwind

declare void @llvm.va_end(i8*) nounwind

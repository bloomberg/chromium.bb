always \x0044\x017E 12456			# LATIN CAPITAL LETTER D + LATIN SMALL LETTER Z WITH CARON
always \x0064\x017E 12456			# LATIN SMALL LETTER D + LATIN SMALL LETTER Z WITH CARON
always LJ 126						# LJ
always Lj 126						# Lj
always lj 126						# lj
always NJ 1246						# NJ
always Nj 1246						# Nj
always nj 1246						# nj

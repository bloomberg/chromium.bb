include es-chardefs.cti

# ---------------------------------------
# Digits
# ---------------------------------------

litdigit 1 1
litdigit 2 12
litdigit 3 14
litdigit 4 145
litdigit 5 15
litdigit 6 124
litdigit 7 1245
litdigit 8 125
litdigit 9 24
litdigit 0 245

# ---------------------------------------
# Braille indicator opcodes
# ---------------------------------------

numsign 3456
capsign 46
begcaps 46-46

# ---------------------------------------
# Translation opcodes
# ---------------------------------------

repeated \s 0
repeated \t 0


module printing.mojom;

import "ui/gfx/geometry/mojo/geometry.mojom";

struct PdfRenderSettings {
  enum Mode {
    NORMAL = 0,
    // Modes below are Windows only.
    TEXTONLY,
    GDI_TEXT,
    POSTSCRIPT_LEVEL2,
    POSTSCRIPT_LEVEL3
  };

  gfx.mojom.Rect area;
  gfx.mojom.Point offsets;
  int32 dpi;
  bool autorotate;
  Mode mode;
};

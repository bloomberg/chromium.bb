    "mixed_content.icon",
    "my_location.icon",
    "navigate_home.icon",
    "navigate_home_touch.icon",
    "navigate_stop.icon",
    "navigate_stop_touch.icon",
    "nfc.icon",
    "overflow_chevron.icon",
    "page_info_content_paste.icon",
    "paintbrush.icon",
    "photo.icon",
    "photo_camera.icon",
    "picture_in_picture_control_background.icon",
    "picture_in_picture_alt.icon",
    "reload_touch.icon",
    "remove.icon",
    "remove_box.icon",
    "resize_handle.icon",
    "sad_tab.icon",
    "security.icon",
    "sensors.icon",
    "settings.icon",
    "signin_button_drop_down_arrow.icon",
    "sign_out.icon",
    "smartphone.icon",
    "speaker.icon",
    "speaker_group.icon",
    "supervisor_account.icon",
    "supervisor_account_circle.icon",
    "sync.icon",
    "sync_circle.icon",
    "sync_error_circle.icon",
    "sync_paused_circle.icon",
    "sync_problem.icon",

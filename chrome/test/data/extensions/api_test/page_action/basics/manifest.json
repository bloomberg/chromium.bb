{
  "name": "A page action with no icon that makes the page red",
  "version": "1.0",
  "manifest_version": 2,
  "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/*"
  ],
  "page_action": {
      "default_title": "Make this page red",
      "default_icon": "icon.png"
  }
}

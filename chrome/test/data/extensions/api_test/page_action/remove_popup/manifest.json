{
  "name": "A page action which removes a popup.",
  "version": "1.0",
  "manifest_version": 2,
  "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/*"
  ],
  "page_action": {
    "default_title": "Popup Title",
    "default_icon": "icon.png",
    "default_popup": "a_popup.html"
  }
}

  js_type_check("closure_compile") {
    deps = [
      ":actions",
      ":app",
      ":browser_proxy",
      ":constants",
      ":fake_page_handler",
      ":item",
      ":main_view",
      ":metadata_view",
      ":pwa_permission_view",
      ":reducers",
      ":router",
      ":store",
      ":store_client",
      ":types",
      ":util",
    ]
  }

  js_library("actions") {
    deps = [
      "//chrome/browser/ui/webui/app_management:mojo_bindings_js_externs",
      "//ui/webui/resources/js:cr",
    ]
  }

  js_library("api_listener") {
    deps = [
      ":actions",
      ":store",
      ":util",
      "//ui/webui/resources/js:cr",
    ]
  }

  js_library("app") {
    deps = [
      ":actions",
      ":browser_proxy",
      ":main_view",
      ":metadata_view",
      ":pwa_permission_view",
      ":router",
      ":util",
    ]
  }

  js_library("browser_proxy") {
    deps = [
      ":fake_page_handler",
      "//chrome/browser/ui/webui/app_management:mojo_bindings_js_externs",
      "//ui/webui/resources/js:cr",
    ]
  }


  ]

  deps = [
    "//base:i18n",
    "//base/test:test_support",
    "//url",
  ]

  dict = "smb_client/fuzzer_data/smb_url_fuzzer.dict"

  seed_corpus = "smb_client/fuzzer_data/smb_url_corpus"
}

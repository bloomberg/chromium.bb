      {
        "type": "object",
        "properties": {
          "kind": {
            "description": "Ali naj se iskanje tiskalnika, ki se ujema, omeji na določeno skupino tiskalnikov.",
            "type": {
              "enum": [ "local", "cloud" ]
            }
          },
          "idPattern": {
            "description": "Regularni izraz, ki se ujema z ID-jem tiskalnika.",
            "type": "string"

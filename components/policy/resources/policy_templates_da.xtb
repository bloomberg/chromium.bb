      {
        "type": "object",
        "properties": {
          "kind": {
            "description": "Hvorvidt søgningen efter en printer, der opfylder attributten, skal begrænses til en specifik gruppe printere.",
            "type": {
              "enum": [ "local", "cloud" ]
            }
          },
          "idPattern": {
            "description": "Regulært udtryk, som matcher printer-id.",
            "type": "string"
          },
          "namePattern": {

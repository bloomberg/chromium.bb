# NOTE: This is a transitional set of build rules so that the internal Mojo
# shuffle can be done in reasonably sized increments.

group("test_support") {
  testonly = true
  public_deps = [
    "//mojo/core/test:test_support",
  ]
}

group("test_support_impl") {
  testonly = true
  public_deps = [
    "//mojo/core/test:test_support_impl",
  ]
}

group("run_all_unittests") {
  testonly = true
  public_deps = [
    "//mojo/core/test:run_all_unittests",
  ]
}
